if ! [ -e output ]; then
	mkdir output
fi

if ! [ -e triples ]; then
	mkdir triples
fi

for repository in $repositories; do
	echo "Running on $repository"
	./codeontology -i $1/$repository -o triples/$repository.nt -vt --explore-dependencies --clean &> output/$repository.out
done
